{
  "name": "ngx-scrollbar",
  "version": "18.0.0",
  "license": "MIT",
  "homepage": "https://ngx-scrollbar.netlify.app/",
  "author": {
    "name": "Murhaf Sousli",
    "url": "http://github.com/murhafsousli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MurhafSousli/ngx-scrollbar"
  },
  "bugs": {
    "url": "http://github.com/murhafsousli/ngx-scrollbar/issues"
  },
  "keywords": [
    "scrollbar",
    "smooth-scroll",
    "scroll-reached"
  ],
  "peerDependencies": {
    "@angular/common": ">=19.0.0",
    "@angular/core": ">=19.0.0",
    "@angular/cdk": ">=19.0.0",
    "rxjs": ">=7.0.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "sideEffects": false,
  "module": "fesm2022/ngx-scrollbar.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "default": "./fesm2022/ngx-scrollbar.mjs"
    },
    "./cdk": {
      "types": "./cdk/index.d.ts",
      "default": "./fesm2022/ngx-scrollbar-cdk.mjs"
    },
    "./mat": {
      "types": "./mat/index.d.ts",
      "default": "./fesm2022/ngx-scrollbar-mat.mjs"
    },
    "./reached-event": {
      "types": "./reached-event/index.d.ts",
      "default": "./fesm2022/ngx-scrollbar-reached-event.mjs"
    },
    "./smooth-scroll": {
      "types": "./smooth-scroll/index.d.ts",
      "default": "./fesm2022/ngx-scrollbar-smooth-scroll.mjs"
    }
  }
}